<?xml version="1.0" encoding="utf-8" standalone="no"?>
<GUI name="machineSettingsAdvanced">
    <GuiElement type="empty" profile="tfMachineDialog_uiInGameMenuFrame">
        <!-- Static page header -->
        <GuiElement type="empty" profile="ingameMenuFrameHeaderPanel">
            <GuiElement type="bitmap" profile="ingameMenuGeneralSettingsHeaderIcon" />
            <GuiElement type="text" profile="tfMachineDialog_ingameMenuFrameHeaderText" text="$l10n_ui_settingsAdvanced" />
        </GuiElement>

        <!-- Layout content box -->
        <GuiElement type="bitmap" profile="tfMachineDialog_ingameMenuSettingsBox" id="settingsContainer">
            <GuiElement type="scrollingLayout" profile="tfMachineDialog_ingameMenuSettingsLayout" id="boxLayout">
                <!-- Area modifiers -->
                <GuiElement type="text" profile="tfMachineDialog_sectionHeader" text="$l10n_ui_areaModifiers" />

                <GuiElement type="empty" profile="tfMachineDialog_inputWrapper">
                    <GuiElement type="machineTextInput" profile="tfMachineDialog_textInput" id="paintModifierOption" onEnterPressed="onEnterPressedInput" name="paintModifier" />
                    <GuiElement type="text" profile="tfMachineDialog_multiTextOptionTitle" text="$l10n_ui_statePaintRadiusModifier" />
                    <GuiElement type="text" profile="tfMachineDialog_multiTextOptionTooltip" text="$l10n_ui_statePaintRadiusModifierTooltip" />
                </GuiElement>

                <GuiElement type="empty" profile="tfMachineDialog_inputWrapper">
                    <GuiElement type="machineTextInput" profile="tfMachineDialog_textInput" id="densityModifierOption" onEnterPressed="onEnterPressedInput" name="densityModifier" />
                    <GuiElement type="text" profile="tfMachineDialog_multiTextOptionTitle" text="$l10n_ui_stateDensityRadiusModifier" />
                    <GuiElement type="text" profile="tfMachineDialog_multiTextOptionTooltip" text="$l10n_ui_stateDensityRadiusModifierTooltip" />
                </GuiElement>


                <!-- Misc -->
                <GuiElement type="text" profile="tfMachineDialog_sectionHeader" text="$l10n_helpLine_Misc" />

                <GuiElement type="empty" profile="tfMachineDialog_inputWrapper">
                    <GuiElement type="machineTextInput" profile="tfMachineDialog_textInput" id="inputRatioOption" onEnterPressed="onEnterPressedInput" name="inputRatio" />
                    <GuiElement type="text" profile="tfMachineDialog_multiTextOptionTitle" text="$l10n_ui_stateInputRatio" />
                    <GuiElement type="text" profile="tfMachineDialog_multiTextOptionTooltip" text="$l10n_ui_stateInputRatioTooltip" />
                </GuiElement>

                <GuiElement type="machineCheckedOption" profile="tfMachineDialog_multiTextOption" onClick="onClickStateCheckedOption" name="enableEffects" id="enableEffectsOption">
                    <GuiElement type="button" profile="tfSettingsDialog_multiTextOptionLeft" />
                    <GuiElement type="button" profile="tfMachineDialog_multiTextOptionRight" />
                    <GuiElement type="text" profile="tfSettingsDialog_multiTextOptionText" />
                    <GuiElement type="text" profile="tfMachineDialog_multiTextOptionTitle" text="$l10n_ui_stateEnableEffects" />
                    <GuiElement type="bitmap" profile="tfSettingsDialog_multiTextOptionBackground" />
                    <GuiElement type="text" profile="tfMachineDialog_multiTextOptionTooltip" text="$l10n_ui_stateEnableEffectsTooltip" />
                    <GuiElement type="bitmap" profile="tfMachineDialog_multiTextOptionFocusBg" />
                </GuiElement>

                <GuiElement type="machineCheckedOption" profile="tfMachineDialog_multiTextOption" onClick="onClickStateCheckedOption" name="allowGradingUp" id="allowGradingUpOption">
                    <GuiElement type="button" profile="tfSettingsDialog_multiTextOptionLeft" />
                    <GuiElement type="button" profile="tfMachineDialog_multiTextOptionRight" />
                    <GuiElement type="text" profile="tfSettingsDialog_multiTextOptionText" />
                    <GuiElement type="text" profile="tfMachineDialog_multiTextOptionTitle" text="$l10n_ui_stateAllowGradingUp" />
                    <GuiElement type="bitmap" profile="tfSettingsDialog_multiTextOptionBackground" />
                    <GuiElement type="text" profile="tfMachineDialog_multiTextOptionTooltip" text="$l10n_ui_stateAllowGradingUpTooltip" />
                    <GuiElement type="bitmap" profile="tfMachineDialog_multiTextOptionFocusBg" />
                </GuiElement>

                <GuiElement type="machineCheckedOption" profile="tfMachineDialog_multiTextOption" onClick="onClickStateCheckedOption" name="forceNodes" id="forceNodesOption">
                    <GuiElement type="button" profile="tfSettingsDialog_multiTextOptionLeft" />
                    <GuiElement type="button" profile="tfMachineDialog_multiTextOptionRight" />
                    <GuiElement type="text" profile="tfSettingsDialog_multiTextOptionText" />
                    <GuiElement type="text" profile="tfMachineDialog_multiTextOptionTitle" text="$l10n_ui_stateForceNodes" />
                    <GuiElement type="bitmap" profile="tfSettingsDialog_multiTextOptionBackground" />
                    <GuiElement type="text" profile="tfMachineDialog_multiTextOptionTooltip" text="$l10n_ui_stateForceNodesTooltip" />
                    <GuiElement type="bitmap" profile="tfMachineDialog_multiTextOptionFocusBg" />
                </GuiElement>

                <!-- Copy state -->
                <GuiElement type="text" profile="tfMachineDialog_sectionHeader" text="$l10n_ui_copySettings" />

                <GuiElement type="empty" profile="tfMachineDialog_inputWrapper">
                    <GuiElement type="button" profile="tfMachineDialog_button" text="$l10n_ui_machines" id="selectMachineButton" onClick="onClickSelectMachine" />
                    <GuiElement type="text" profile="tfMachineDialog_multiTextOptionTitle" text="$l10n_ui_copySettingsTooltip" />
                </GuiElement>


            </GuiElement>
        </GuiElement>
    </GuiElement>
</GUI>