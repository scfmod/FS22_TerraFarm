                <GuiElement type="machineCheckedOption" profile="tfMachineDialog_multiTextOption" onClick="onClickStateCheckedOption" name="removeFieldArea" id="removeFieldAreaOption">
                    <GuiElement type="button" profile="tfSettingsDialog_multiTextOptionLeft" />
                    <GuiElement type="button" profile="tfMachineDialog_multiTextOptionRight" />
                    <GuiElement type="text" profile="tfSettingsDialog_multiTextOptionText" />
                    <GuiElement type="text" profile="tfMachineDialog_multiTextOptionTitle" text="$l10n_ui_stateRemoveFieldArea" />
                    <GuiElement type="bitmap" profile="tfSettingsDialog_multiTextOptionBackground" />
                    <GuiElement type="text" profile="tfMachineDialog_multiTextOptionTooltip" text="$l10n_ui_stateRemoveFieldAreaTooltip" />
                    <GuiElement type="bitmap" profile="tfMachineDialog_multiTextOptionFocusBg" />
                </GuiElement>

                <GuiElement type="machineCheckedOption" profile="tfMachineDialog_multiTextOption" onClick="onClickStateCheckedOption" name="removeStoneArea" id="removeStoneAreaOption">
                    <GuiElement type="button" profile="tfSettingsDialog_multiTextOptionLeft" />
