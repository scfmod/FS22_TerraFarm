<?xml version="1.0" encoding="utf-8" standalone="no"?>
<GUI name="machineSettingsLandscaping">
    <GuiElement type="empty" profile="tfMachineDialog_uiInGameMenuFrame">
        <!-- Static page header -->
        <GuiElement type="empty" profile="ingameMenuFrameHeaderPanel">
            <GuiElement type="bitmap" profile="ingameMenuGeneralSettingsHeaderIcon" />
            <GuiElement type="text" profile="tfMachineDialog_ingameMenuFrameHeaderText" text="$l10n_ui_landscaping" />
        </GuiElement>

        <!-- Layout content box -->
        <GuiElement type="bitmap" profile="tfMachineDialog_ingameMenuSettingsBox" id="settingsContainer">
            <GuiElement type="scrollingLayout" profile="tfMachineDialog_ingameMenuSettingsLayout" id="boxLayout">
                <GuiElement type="text" profile="tfMachineDialog_sectionHeader" text="$l10n_ui_postDeformation" />

                <GuiElement type="machineCheckedOption" profile="tfMachineDialog_multiTextOption" onClick="onClickStateCheckedOption" name="clearDecoArea" id="clearDecoAreaOption">
                    <GuiElement type="button" profile="tfSettingsDialog_multiTextOptionLeft" />
                    <GuiElement type="button" profile="tfMachineDialog_multiTextOptionRight" />
                    <GuiElement type="text" profile="tfSettingsDialog_multiTextOptionText" />
                    <GuiElement type="text" profile="tfMachineDialog_multiTextOptionTitle" text="$l10n_ui_stateClearDecoArea" />
                    <GuiElement type="bitmap" profile="tfSettingsDialog_multiTextOptionBackground" />
                    <GuiElement type="text" profile="tfMachineDialog_multiTextOptionTooltip" text="$l10n_ui_stateClearDecoAreaTooltip" />
                    <GuiElement type="bitmap" profile="tfMachineDialog_multiTextOptionFocusBg" />
                </GuiElement>

                <GuiElement type="machineCheckedOption" profile="tfMachineDialog_multiTextOption" onClick="onClickStateCheckedOption" name="clearDensityMapHeightArea" id="clearDensityMapHeightAreaOption">
                    <GuiElement type="button" profile="tfSettingsDialog_multiTextOptionLeft" />
                    <GuiElement type="button" profile="tfMachineDialog_multiTextOptionRight" />
                    <GuiElement type="text" profile="tfSettingsDialog_multiTextOptionText" />
                    <GuiElement type="text" profile="tfMachineDialog_multiTextOptionTitle" text="$l10n_ui_stateClearDensityMapHeightArea" />
                    <GuiElement type="bitmap" profile="tfSettingsDialog_multiTextOptionBackground" />
                    <GuiElement type="text" profile="tfMachineDialog_multiTextOptionTooltip" text="$l10n_ui_stateClearDensityMapHeightAreaTooltip" />
                    <GuiElement type="bitmap" profile="tfMachineDialog_multiTextOptionFocusBg" />
                </GuiElement>

                <GuiElement type="machineCheckedOption" profile="tfMachineDialog_multiTextOption" onClick="onClickStateCheckedOption" name="eraseTireTracks" id="eraseTireTracksOption">
                    <GuiElement type="button" profile="tfSettingsDialog_multiTextOptionLeft" />
                    <GuiElement type="button" profile="tfMachineDialog_multiTextOptionRight" />
                    <GuiElement type="text" profile="tfSettingsDialog_multiTextOptionText" />
                    <GuiElement type="text" profile="tfMachineDialog_multiTextOptionTitle" text="$l10n_ui_stateEraseTireTracks" />
                    <GuiElement type="bitmap" profile="tfSettingsDialog_multiTextOptionBackground" />
                    <GuiElement type="text" profile="tfMachineDialog_multiTextOptionTooltip" text="$l10n_ui_stateEraseTireTracksTooltip" />
                    <GuiElement type="bitmap" profile="tfMachineDialog_multiTextOptionFocusBg" />
