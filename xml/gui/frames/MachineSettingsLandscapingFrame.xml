            </GuiElement>

            <GuiElement type="threePartBitmap" profile="tfMachineDialog_listVerticalSlider">
                <GuiElement type="slider" profile="tfMachineDialog_listVerticalSliderBar" dataElementId="boxLayout" handleFocus="false" />
            </GuiElement>
        </GuiElement>
    </GuiElement>
