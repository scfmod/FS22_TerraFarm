                    <GuiElement type="text" profile="tfMachineDialog_multiTextOptionTitle" text="$l10n_ui_stateRemoveWeedArea" />
                    <GuiElement type="bitmap" profile="tfSettingsDialog_multiTextOptionBackground" />
                    <GuiElement type="text" profile="tfMachineDialog_multiTextOptionTooltip" text="$l10n_ui_stateRemoveWeedAreaTooltip" />
                    <GuiElement type="bitmap" profile="tfMachineDialog_multiTextOptionFocusBg" />
